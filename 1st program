// program is to find posibble of x in a quadratic equation.
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float b,a,c,x1,x2;
    cout<<"b value";
    cin>>b;
    cout<<"c value";
    cin>>c;
    cout<<"a value";
    cin>>a;
    x1=(-b+sqrt(b*b-4*a*c))/(2*a);
    x2=(-b-sqrt(b*b-4*a*c))/(2*a);
    cout<<"x1: "<<x1<<endl;
    cout<<"x2: "<<x2;
    return 0;
}
